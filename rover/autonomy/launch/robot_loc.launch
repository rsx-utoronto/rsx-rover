        <param name="imu1_queue_size" value="10" />
        <param name="imu1_remove_gravitational_acceleration" value="true" />
        <rosparam param="imu1_config">[false,  false, false,
                                       false, false, false,
                                       false, false, false,
                                       true , true, true,
                                       true, true, true]</rosparam> -->

