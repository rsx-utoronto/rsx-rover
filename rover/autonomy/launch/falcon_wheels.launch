<launch>
    <rosparam command="load" file="$(find rover)/drive/launch/goose.yaml" />
    <node pkg="ros_phoenix" type="ros_phoenix_node" name="ros_phoenix" respawn="false" />
    <node pkg="ros_phoenix" type="falcon_motor_control_node" name="falcon_motor_control" respawn="false" />
    <node pkg="ros_phoenix" type="wheel_node" name="wheel_node">
        <param name="motor_name" value="mid_left"/>
    </node>
    <node pkg="ros_phoenix" type="wheel_node" name="wheel_node">
        <param name="motor_name" value="mid_right"/>
    </node>
    <node pkg='rsx-rover' type='wheel_odometry_node' name='wheel_odometry_node'/>
</launch>