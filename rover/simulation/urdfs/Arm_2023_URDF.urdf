<?xml version="1.0"?><robot name="full_arm_robot">
  	<material name="grey">
      <color rgba="0.5 0.5 0.5 1"/>
  	</material>
  
  	<material name="blue">
      <color rgba="0 0.1 0.4 1"/>
  	</material>
  
	<link name="base_link">
  		<visual>
          <geometry>
            <box size="0.15 0.3 0.15"/>
          </geometry>
          <origin xyz="0 -0.4 0"/>
          <material name="grey"/>
  		</visual>
  	</link>

	<link name="rotate_1">
		<visual>
          <geometry>
            <cylinder length="0.08" radius="0.03"/>
          </geometry>
          <origin rpy="0 1.57 0" xyz="0 0 0"/>
          <material name="blue"/>
        </visual>  
	</link>

	<link name="upper_arm">
		<visual>
          <geometry>
            <box size="0.05 0.5 0.1"/>
          </geometry>
          <origin xyz="0 0.165 0.12" rpy="0.6 0 0"/>
          <material name="grey"/>
		</visual>
	</link>

	<link name="rotate_2">
		<visual>
          <geometry>
            <cylinder length="0.13" radius="0.03"/>
          </geometry>
          <origin rpy="0 1.57 0" xyz="-0.02 0.32 0.225"/>
          <material name="blue"/>
		</visual>
	</link>

	<link name="forearm">
		<visual>
          <geometry>
            <box size="0.05 0.5 0.1"/>
          </geometry>
          <origin xyz="-0.025 0.195 -0.07" rpy="-0.4 0 0"/>
          <material name="grey"/>
		</visual>
	</link>

	<link name="wrist">
		<visual>
          <geometry>
            <cylinder length="0.17" radius="0.075"/>
          </geometry>
          <origin xyz="-0.03 0.35 -0.145" rpy="1.21 0 0"/>
          <material name="blue"/>
</visual>
</link>

	<joint name="forearm_to_rotate_2" type="continuous">
		<parent link="upper_arm"/>
    	<child link="forearm"/>
      	<origin xyz="-0.02 0.32 0.225" rpy="0 0 0"/>
      	<limit effort="1000.0" lower="-1.34" upper="0.8" velocity="0.5"/>
        <axis xyz="1 0 0"/>
	</joint>

	<joint name="forearm_to_rotate_2b" type="continuous">
      <parent link="upper_arm"/>
    	<child link="forearm"/>
      	<limit effort="1000.0" lower="-0.3" upper="0.548" velocity="0.5"/>
      	<origin xyz="-0.02 0.32 0.225" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
	</joint>

	<joint name="upper_arm_to_rotate_1" type="continuous">
		<parent link="rotate_1"/>
      	<child link="upper_arm"/>
        <limit effort="1000.0" lower="-0.44" upper="2.54" velocity="0.5"/>
      	<origin xyz="0 -0.35 0.12" rpy="0 0 0"/>
      	<axis xyz="1 0 0"/>
	</joint>

	<joint name="upper_arm_to_rotate_1b" type="fixed">
		<parent link="upper_arm"/>
      	<child link="rotate_1"/>
	</joint>
	
	<joint name="upper_arm_to_rotate_2" type="fixed">
		<parent link="upper_arm"/>
      	<child link="rotate_2"/>
	</joint>

	<joint name="forearm_to_wrist" type="fixed">
      	<parent link="forearm"/>
      	<child link="wrist"/>
      </joint>

	<joint name="base_rotate" type="continuous">
      	<parent link="base_link"/>
      	<child link="upper_arm"/>
		<origin xyz="0 -0.345 0.135" rpy="0 0 0"/>
      	<axis xyz="0 0 1"/>
	</joint>

</robot>
