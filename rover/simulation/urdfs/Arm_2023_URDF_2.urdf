      	<axis xyz="1 0 0"/>
	</joint-->
	
	<joint name="joint_1c" type="fixed">
		<parent link="upper_arm"/>
      	<child link="rotate_2"/>
      	<origin rpy="0 0 0" xyz="-0.02 0.4 0"/>
	</joint>

</robot>
