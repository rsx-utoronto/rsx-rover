#~/.config/systemd/user/zenoh.service

[Unit]
Description=Start Zenoh Router
Wants=network-online.target
After=network-online.target

[Service]
ExecStart=bash -c "source /opt/ros/humble/setup.bash && ROS_DOMAIN_ID=10 RMW_IMPLEMENTATION=rmw_zenoh_cpp ZENOH_ROUTER_CONFIG_URI=/home/rsx-base/rover_ws/src/rsx-rover/config/zenoh_base.json5 ros2 run rmw_zenoh_cpp rmw_zenohd"
Type=simple
Restart=on-failure

[Install]
WantedBy=default.target

